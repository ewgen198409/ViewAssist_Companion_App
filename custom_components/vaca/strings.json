{
  "config": {
    "step": {
      "user": {
        "data": {
          "host": "[%key:common::config_flow::data::host%]",
          "port": "[%key:common::config_flow::data::port%]"
        }
      },
      "hassio_confirm": {
        "description": "[%key:config::step::hassio_confirm::description%]"
      },
      "zeroconf_confirm": {
        "description": "[%key:config::step::zeroconf_confirm::description%]",
        "title": "[%key:config::step::zeroconf_confirm::title%]"
      }
    },
    "error": {
      "cannot_connect": "[%key:common::config_flow::error::cannot_connect%]"
    },
    "abort": {
      "already_configured": "[%key:common::config_flow::abort::already_configured_service%]",
      "no_services": "[%key:config::abort::no_services%]",
      "no_port": "[%key:config::abort::no_port%]"
    }
  },
  "entity": {
    "binary_sensor": {
      "assist_in_progress": {
        "name": "[%key:component::assist_pipeline::entity::binary_sensor::assist_in_progress::name%]"
      }
    },
    "select": {
      "pipeline": {
        "name": "[%key:component::assist_pipeline::entity::select::pipeline::name%]",
        "state": {
          "preferred": "[%key:component::assist_pipeline::entity::select::pipeline::state::preferred%]"
        }
      },
      "noise_suppression_level": {
        "name": "[%key:entity::select::noise_suppression_level::name%]",
        "state": {
          "off": "[%key:common::state::off%]",
          "low": "[%key:entity::select::noise_suppression_level::state::low%]",
          "medium": "[%key:entity::select::noise_suppression_level::state::medium%]",
          "high": "[%key:entity::select::noise_suppression_level::state::high%]",
          "max": "[%key:entity::select::noise_suppression_level::state::max%]"
        }
      },
      "vad_sensitivity": {
        "name": "[%key:component::assist_pipeline::entity::select::vad_sensitivity::name%]",
        "state": {
          "default": "[%key:component::assist_pipeline::entity::select::vad_sensitivity::state::default%]",
          "aggressive": "[%key:component::assist_pipeline::entity::select::vad_sensitivity::state::aggressive%]",
          "relaxed": "[%key:component::assist_pipeline::entity::select::vad_sensitivity::state::relaxed%]"
        }
      }
    },
    "switch": {
      "mute": {
        "name": "[%key:entity::switch::mute::name%]"
      }
    },
    "number": {
      "mic_gain": {
        "name": "[%key:entity::number::mic_gain::name%]"
      },
      "notification_volume": {
        "name": "[%key:entity::number::notification_volume::name%]"
      },
      "screen_brightness": {
        "name": "[%key:entity::number::screen_brightness::name%]"
      }
    }
  }
}
